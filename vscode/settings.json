{
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "files.insertFinalNewline": true,
  "workbench.colorCustomizations": {
    "gitDecoration.ignoredResourceForeground": "#6b6d68",
    "gitDecoration.modifiedResourceForeground": "#d7b968",
    "gitDecoration.deletedResourceForeground": "#f65",
    "gitDecoration.untrackedResourceForeground": "#6eca8d"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/*.map": true
  },
  "terminal.integrated.shell.linux": "/bin/zsh",
  "terminal.integrated.fontFamily": "Menlo for Powerline",
  "workbench.iconTheme": "vscode-great-icons",
  "coverage-gutters.lcovname": "coverage/lcov.info",
  "coverage-gutters.showLineCoverage": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": false,
  "editor.renderControlCharacters": false,
  "gitlens.advanced.messages": {
    "suppressCommitNotFoundWarning": true
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false
}
