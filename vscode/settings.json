{
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "files.insertFinalNewline": true,
  "workbench.colorCustomizations": {
    "gitDecoration.ignoredResourceForeground": "#6b6d68",
    "gitDecoration.modifiedResourceForeground": "#d7b968",
    "gitDecoration.deletedResourceForeground": "#f65",
    "gitDecoration.untrackedResourceForeground": "#6eca8d"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/lib": true,
    "**/*.map": true
  },
  "terminal.integrated.fontFamily": "Menlo for Powerline",
  "workbench.iconTheme": "vscode-great-icons",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": false,
  "editor.renderControlCharacters": false,
  "gitlens.advanced.messages": {
    "suppressCommitNotFoundWarning": true
  },
  "telemetry.telemetryLevel": "off",
  "redhat.telemetry.enabled": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "docker.showStartPage": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
